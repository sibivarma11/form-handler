{
    "name": "sibivarma/form-submissions",
    "description": "A Laravel package for handling form submissions with email notifications and Filament admin interface",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sibi Varma",
            "email": "sibivarma11@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "filament/filament": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SibiVarma\\FormSubmissions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SibiVarma\\FormSubmissions\\FormSubmissionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}